<div id="outline-container-org020c1e5" class="outline-2">
<h2 id="org020c1e5">Basic Queries (1:56:11)</h2>
<div class="outline-text-2" id="text-org020c1e5">
<div class="org-src-container">
<pre class="src src-sql"><span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> student.<span style="color: #3a81c3; font-weight: bold;">name</span>, student.major
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> student.<span style="color: #3a81c3; font-weight: bold;">name</span>, student.major
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">ORDER</span> <span style="color: #3a81c3; font-weight: bold;">BY</span> <span style="color: #3a81c3; font-weight: bold;">name</span>;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> student.<span style="color: #3a81c3; font-weight: bold;">name</span>, student.major
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">ORDER</span> <span style="color: #3a81c3; font-weight: bold;">BY</span> <span style="color: #3a81c3; font-weight: bold;">name</span> <span style="color: #3a81c3; font-weight: bold;">DESC</span>;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> student.<span style="color: #3a81c3; font-weight: bold;">name</span>, student.major
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">ORDER</span> <span style="color: #3a81c3; font-weight: bold;">BY</span> major <span style="color: #3a81c3; font-weight: bold;">name</span>  <span style="color: #3a81c3; font-weight: bold;">ASC</span>;

<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">Limit n : show only the top n result </span>
<span style="color: #3a81c3; font-weight: bold;">SELECT</span> student.<span style="color: #3a81c3; font-weight: bold;">name</span>, student.major
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">ORDER</span> <span style="color: #3a81c3; font-weight: bold;">BY</span> major <span style="color: #3a81c3; font-weight: bold;">name</span>  <span style="color: #3a81c3; font-weight: bold;">ASC</span>;
<span style="color: #3a81c3; font-weight: bold;">LIMIT</span> 2;

<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">WHERE</span>
<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">Condtions in where are</span>
<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">[ &lt;, &gt;, &lt;=, &gt;=, =,&lt;&gt;(NOT Equal), AND, OR]</span>

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> <span style="color: #3a81c3; font-weight: bold;">name</span> = <span style="color: #2d9574;">'Jack'</span>;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> student_id &gt; 2;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">show all student whoes id is &lt; 2 and don't include Jack </span>
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> student_id &lt;= 2 <span style="color: #3a81c3; font-weight: bold;">AND</span> <span style="color: #3a81c3; font-weight: bold;">name</span> &lt;&gt; <span style="color: #2d9574;">'Jack'</span>;

<span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> major = <span style="color: #2d9574;">'Biology'</span> <span style="color: #3a81c3; font-weight: bold;">AND</span> student_id &gt; 1;
<span style="color: #2aa1ae; background-color: #ecf3ec;">-- </span><span style="color: #2aa1ae; background-color: #ecf3ec;">IN</span>
<span style="color: #3a81c3; font-weight: bold;">SELECT</span> *
<span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> major <span style="color: #3a81c3; font-weight: bold;">IN</span> (<span style="color: #2d9574;">'Biology'</span>, <span style="color: #2d9574;">'Chemistry'</span>) <span style="color: #3a81c3; font-weight: bold;">AND</span> student_id &gt;2;

</pre>
</div>
</div>
</div>
