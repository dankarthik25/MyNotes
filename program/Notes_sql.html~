<span style="color: #3a81c3; font-weight: bold;">INSERT</span> <span style="color: #3a81c3; font-weight: bold;">INTO</span> works_with <span style="color: #3a81c3; font-weight: bold;">VALUES</span>(102, 406, 15000);
