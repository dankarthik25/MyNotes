<div id="outline-container-org17640ed" class="outline-2">
<h2 id="org17640ed">Update &amp; Delete (1:48:11)</h2>
<div class="outline-text-2" id="text-org17640ed">
</div>
<div id="outline-container-org5a8cb80" class="outline-3">
<h3 id="org5a8cb80">Update</h3>
<div class="outline-text-3" id="text-org5a8cb80">
<div class="org-src-container">
<pre class="src src-sql"><span style="color: #3a81c3; font-weight: bold;">UPDATE</span> student
<span style="color: #3a81c3; font-weight: bold;">SET</span> major = <span style="color: #2d9574;">'Undecided'</span>;

<span style="color: #3a81c3; font-weight: bold;">UPDATE</span> student
<span style="color: #3a81c3; font-weight: bold;">SET</span> <span style="color: #3a81c3; font-weight: bold;">name</span> = <span style="color: #2d9574;">'Johnny'</span>
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> student_id = 4;

<span style="color: #3a81c3; font-weight: bold;">UPDATE</span> student
<span style="color: #3a81c3; font-weight: bold;">SET</span> major = <span style="color: #2d9574;">'Biological Sciences'</span>
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> major = <span style="color: #2d9574;">'Biology'</span>;

<span style="color: #3a81c3; font-weight: bold;">UPDATE</span> student
<span style="color: #3a81c3; font-weight: bold;">SET</span> major = <span style="color: #2d9574;">'Biosociology'</span>
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> major = <span style="color: #2d9574;">'Biology'</span> <span style="color: #3a81c3; font-weight: bold;">OR</span> major = <span style="color: #2d9574;">'sociology'</span>

<span style="color: #3a81c3; font-weight: bold;">UPDATE</span> student
<span style="color: #3a81c3; font-weight: bold;">SET</span> major = <span style="color: #2d9574;">'Undecided'</span>, <span style="color: #3a81c3; font-weight: bold;">name</span> = <span style="color: #2d9574;">'Tom'</span>
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> student_id = 4;

</pre>
</div>
</div>
</div>
<div id="outline-container-org82ff497" class="outline-3">
<h3 id="org82ff497">Delete</h3>
<div class="outline-text-3" id="text-org82ff497">
<div class="org-src-container">
<pre class="src src-sql"><span style="color: #3a81c3; font-weight: bold;">DELETE</span> <span style="color: #3a81c3; font-weight: bold;">FROM</span> student;

<span style="color: #3a81c3; font-weight: bold;">DELETE</span> <span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> student_id = 4;

<span style="color: #3a81c3; font-weight: bold;">DELETE</span> <span style="color: #3a81c3; font-weight: bold;">FROM</span> student
<span style="color: #3a81c3; font-weight: bold;">WHERE</span> major = <span style="color: #2d9574;">'Sociology'</span> <span style="color: #3a81c3; font-weight: bold;">AND</span> <span style="color: #3a81c3; font-weight: bold;">name</span> = <span style="color: #2d9574;">'Kate'</span>;


</pre>
</div>
</div>
</div>
